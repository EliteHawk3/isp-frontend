    "@react-navigation/stack": "^7.0.14",
    "axios": "^1.7.8",
    "expo": "^52.0.1",
    "expo-linear-gradient": "~14.0.1",
    "expo-updates": "~0.26.9",
    "lodash.pick": "^4.4.0",
    "lottie-react-native": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.76.3",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "^4.14.0",
