    "eslint": "^8.57.1",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^2.8.8",
    "react-native-reanimated": "^3.16.1",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.7.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "engines": {
    "node": ">=18"
